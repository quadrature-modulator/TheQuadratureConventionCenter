{
	"name": "TheQuadratureConventionCenter",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			},
			"TS": {
				"$path": "out/shared"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"$properties": {
				"CharacterUseJumpPower": false,
				"CameraMaxZoomDistance": 128
			},
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"TS": {
					"$path": "out/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true,
				"PhysicsSteppingMethod": "Fixed",
				"StreamingEnabled": true,
				"StreamingIntegrityMode": "PauseOutsideLoadedArea",
				"StreamingMinRadius": 500,
				"StreamingTargetRadius": 500,
				"ModelStreamingBehavior": "Improved",
				"StreamOutBehavior": "Opportunistic"
			},
			"Zone0": {"$path": "models/welcome-zone.rbxm"},
			"Zone255": {"$path": "models/test-zone.rbxm"}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"Lighting": {"$path": "models/lighting.model.json"},
		"TextChatService": {
			"$className": "TextChatService",
			"$properties": {
				"ChatVersion": "TextChatService"
			}
		},
		"MaterialService": {
			"$className": "MaterialService",
			"$properties": {
				"Use2022Materials": true
			}
		}
	}
}
